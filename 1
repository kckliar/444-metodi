mono=sqrt(x) - exp(x);
x_val=[1;11;4;35;41;88]
y_val= sqrt(x_val) - exp(x_val);
plot(x_val,y_val,'ro') 
hold on
plot(x,mono,'r--');
err=abs(mono-y1');
