x=linspace(1,100,100);
mono=sqrt(x);
x_val=[1;4;9;16;25;36;81;100]
y_val= sqrt(x_val);
plot(x_val,y_val,'ro') 
hold on
plot(x,mono,'r--');
err=abs(mono-fx2');
errSKO=sqrt(sum((err/(max(mono)-min(mono))).^2)/100)
errMAX=max(err/(max(mono)-min(mono)))

