x1=linspace(1,10,100);
x2=linspace(1,100,100);
mono1=sqrt(x);
nemono1 = cos(x);
neglad1=sign(x-5);
%Graph with standard functions 
figure;
plot(x1,mono1,'k','LineWidth',1); hold on
plot(x1,nemono1,':*b','LineWidth',1);
plot(x1,neglad1,'-.rs','LineWidth',1);
legend('Монотонная','Немонотонная','Негладкая'); 
