function y=myfun(x)
y=(1+25*x.*x).^-0.5;


n=7 или 9 или 11;
a=-1;
b=1;
i=0:n-1;
x_valCH=(b+a)/2 + 0.5*(b-a)*cos(pi*(2*i+1)/(2*n));
y_valCH=myfun(x_valCH);
x_val=linspace(a,b,n);
y_val=myfun(x_val);
x=linspace(a,b,100);
y=myfun(x);
plot(x_val,y_val,'ro',x_valCH,y_valCH,'bo')
grid on
hold on
plot(x,y,'r--')

errob=abs(y-y9');

errMAXob=max(errob/(max(y)-min(y)))

err4=abs(y_valCH-y10');
errMAX4=max(err4/(max(y_valCH)-min(y_valCH)))



