format long
f =@(x)(log(x))*(x.^-1);
x1=exp(2);
x2=exp(6);
n=100;
h=(x2-x1)/n;

% прямоуг лев
s=0;
for i = 0:n-1
 x = x1 + i*h;
 s = s + f(x);
end
I0 = h*s

% прямоуг прав
s=0;
for i = 1:n
 x = x1 + i*h;
 s = s + f(x);
end
I00 = h*s
 
% средниx
s=0;
for x = x1 + h/2:h:x2;
 s=s+f(x);
end
I1= h * s
% трапеции
s=0;
for i = 1:n-1
 x = x1 + i*h;
 s = s + f(x);
end
I2 = h*(s+0.5*f(x1)+0.5*f(x2))

1.2 
m1 = max(f'(x))
[a;b]

n>M1(b-a)^2/(2epselun) 
M1(b-a)^2/(2epselun) = 972255
h=(b-a)/n


